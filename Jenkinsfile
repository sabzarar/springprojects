pipeline {
    agent any
    stages {
        stage('Init') {
            steps {
                echo 'Hi, This is Sabzar from Evry Tieto'
                echo 'Initializing the project'
            }
        }
        stage('build') {
            steps {
                echo 'Building sample maven project'
            }
        }
        stage('deploy') {
            steps {
                echo 'Deploying the project'
            }
        }

    }
}